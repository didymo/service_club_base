{
    "name": "didymo/service_club_base",
    "description": "The base website for the Service Club Project.",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Dominic Di Pietro",
            "role": "Developer"
        },
        {
            "name": "Maxwell Lill",
            "role": "Developer"
        },
        {
            "name": "Tahlya Maling",
            "role": "Developer"
        },
        {
            "name": "Timothy McCaul",
            "role": "Developer"
        },
        {
            "name": "Jack Wallace",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "didymo/service_club_install_profile",
                "version": "8.0.0",
                "type": "drupal-profile",
                "source": {
                    "url": "https://github.com/didymo/service_club_install_profile.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "didymo/service_club_profile",
                "version": "8.0.0",
                "type": "drupal-module",
                "source": {
                    "url": "https://github.com/didymo/service_club_profile.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "didymo/service_club_asset",
                "version": "8.0.0",
                "type": "drupal-module",
                "source": {
                    "url": "https://github.com/didymo/service_club_asset.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "didymo/service_club_event",
                "version": "8.0.0",
                "type": "drupal-module",
                "source": {
                    "url": "https://github.com/didymo/service_club_event.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "didymo/service_club_tmp",
                "version": "8.0.0",
                "type": "drupal-module",
                "source": {
                    "url": "https://github.com/didymo/service_club_tmp.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
	{
            "type": "package",
            "package": {
                "name": "didymo/service_club_theme",
                "version": "8.0.0",
                "type": "drupal-theme",
                "source": {
                    "url": "https://github.com/didymo/service_club_theme",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/console": "^1.0.2",
        "drupal/core": "~8.5.3",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "drupal/address": "~1.0",
        "drupal/geocoder": "^2.0",
        "drupal/geofield": "^1.0",
        "drupal/geolocation": "^1.11",
        "drupal/inline_entity_form": "^1.0",
        "drupal/admin_toolbar": "^1.24",
        "drupal/calendar": "1.x-dev",
        "drupal/ctools": "^3.0",
        "drupal/dynamic_entity_reference": "^1.5",
        "drupal/front": "^1.0",
        "drupal/mailsystem": "^4.1",
        "drupal/token": "^1.3",
        "drupal/pathauto": "^1.2",
        "drupal/profile": "^1.0",
        "drupal/redirect": "^1.2",
        "drupal/scheduler": "^1.0",
        "drupal/simple_gmap": "^1.4",
        "drupal/simplify": "^1.0",
        "drupal/restui": "^1.15",
        "drupal/bootstrap":"^3.11",
        "drupal/mail_login":"^1.0",
        "didymo/service_club_install_profile": "*",
        "didymo/service_club_profile": "*",
        "didymo/service_club_asset": "*",
        "didymo/service_club_event": "*",
        "didymo/service_club_tmp": "*",
        "didymo/service_club_theme": "*"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "~8.5.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        }
    }
}
